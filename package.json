{
   "name": "sbis3-builder",
   "version": "3.18.400-BUILD",
   "private": true,
   "description": "Grunt-based builder for SBIS3 web projects / Gulp-based builder for SBIS3 web projects",
   "files": [
      "Gruntfile.js",
      "gulpfile.js",
      "gulp/",
      "lib/",
      "packer/",
      "resources/",
      "Tasks/"
   ],
   "scripts": {
      "build": "npm run verify && npm run build-only",
      "verify": "npm run lint && npm test",
      "build-only": "node copy-to-dest.js && cd dest && npm i --production --legacy-bundling=false",
      "test": "node ./node_modules/mocha/bin/mocha --timeout 120000 -R xunit --reporter-options output=xunit-result.xml > xunit.log",
      "lint": "node ./node_modules/eslint/bin/eslint --no-color --no-eslintrc -c=.eslintrc -f=stylish .",
      "fix": "node ./node_modules/eslint/bin/eslint --no-eslintrc -c=.eslintrc --fix .",
      "coverage": "node ./node_modules/nyc/bin/nyc --reporter=html --all ./node_modules/.bin/mocha --timeout 120000 -R xunit --reporter-options output=xunit-result.xml > xunit.log"
   },
   "dependencies": {
      "async": "2.6.1",
      "autoprefixer": "8.5.1",
      "clean-css": "4.1.11",
      "escodegen": "1.9.1",
      "esprima": "4.0.0",
      "estraverse": "4.2.0",
      "fs-extra": "6.0.1",
      "grunt": "1.0.2",
      "grunt-cli": "1.2.0",
      "grunt-text-replace": "0.4.0",
      "gulp": "4.0.0",
      "gulp-chmod": "2.0.0",
      "gulp-if": "2.0.2",
      "gulp-plumber": "1.2.0",
      "gulp-rename": "1.2.3",
      "humanize": "0.0.9",
      "less": "3.0.4",
      "minimatch": "3.0.4",
      "p-map": "1.2.0",
      "postcss": "6.0.22",
      "postcss-safe-parser": "3.0.1",
      "postcss-url": "7.3.2",
      "requirejs": "2.3.5",
      "sbis3-controls": "git+ssh://git@git.sbis.ru:sbis/controls.git#rc-3.18.400",
      "sbis3-ws": "git+ssh://git@git.sbis.ru:sbis/ws.git#rc-3.18.400",
      "strip-bom": "3.0.0",
      "tensor-xmldom": "0.2.0-b9",
      "through2": "2.0.3",
      "uglify-js": "3.3.23",
      "vinyl": "2.1.0",
      "workerpool": "2.3.0",
      "ws-data": "git+ssh://git@git.sbis.ru:ws/data.git#rc-3.18.400"
   },
   "devDependencies": {
      "@tensor-corp/eslint-config": "2.0.1",
      "chai": "4.1.2",
      "chai-as-promised": "7.1.1",
      "eslint": "4.19.1",
      "eslint-plugin-eslint-comments": "2.0.2",
      "eslint-plugin-filenames": "1.2.0",
      "eslint-plugin-node": "6.0.1",
      "eslint-plugin-promise": "3.8.0",
      "mocha": "5.2.0",
      "nyc": "11.8.0",
      "sbis3-json-generator": "git+ssh://git@git.sbis.ru:root/sbis3-json-generator.git#rc-3.18.400"
   },
   "engines": {
      "node": ">=8.9.3"
   }
}