{
   "name": "sbis3-builder",
   "version": "3.18.350-BUILD",
   "private": true,
   "description": "Grunt-based builder for SBIS3 web projects / Gulp-based builder for SBIS3 web projects",
   "files": [
      "Gruntfile.js",
      "gulpfile.js",
      "gulp/",
      "lib/",
      "packer/",
      "resources/",
      "Tasks/"
   ],
   "scripts": {
      "build": "npm run verify && npm run build-only",
      "verify": "npm run lint && npm test",
      "build-only": "node copy-to-dest.js && cd dest && npm i --production --legacy-bundling=false",
      "test": "node ./node_modules/mocha/bin/mocha --timeout 60000 -R xunit --reporter-options output=xunit-result.xml > xunit.log",
      "lint": "node ./node_modules/eslint/bin/eslint --no-color --no-eslintrc -c=.eslintrc -f=stylish .",
      "fix": "node ./node_modules/eslint/bin/eslint --no-eslintrc -c=.eslintrc --fix ."
   },
   "dependencies": {
      "async": "^2.6.0",
      "autoprefixer": "^8.2.0",
      "sbis3-controls": "git+ssh://git@git.sbis.ru:sbis/controls.git#rc-3.18.300",
      "clean-css": "^4.1.11",
      "escodegen": "^1.8.1",
      "esprima": "^4.0.0",
      "estraverse": "^4.2.0",
      "fs-extra": "^5.0.0",
      "grunt": "^1.0.2",
      "grunt-cli": "^1.2.0",
      "grunt-text-replace": "^0.4.0",
      "gulp": "^4.0.0",
      "gulp-chmod": "^2.0.0",
      "gulp-if": "^2.0.2",
      "gulp-plumber": "^1.2.0",
      "gulp-rename": "^1.2.2",
      "humanize": "0.0.9",
      "less": "^3.0.1",
      "minimatch": "^3.0.4",
      "postcss": "^6.0.21",
      "postcss-safe-parser": "^3.0.1",
      "postcss-url": "^7.3.2",
      "requirejs": "^2.3.5",
      "p-map": "^1.2.0",
      "sbis3-ws": "git+ssh://git@git.sbis.ru:sbis/ws.git#rc-3.18.350",
      "strip-bom": "^3.0.0",
      "tensor-xmldom": "0.2.0-b9",
      "through2": "^2.0.3",
      "uglify-js": "3.3.23",
      "vinyl": "^2.1.0",
      "workerpool": "^2.3.0",
"ws-data": "git+ssh://git@git.sbis.ru:ws/data.git#rc-3.18.300"   },
   "devDependencies": {
      "chai": "^4.1.2",
      "chai-as-promised": "^7.1.1",
      "eslint": "^4.16.0",
      "eslint-plugin-eslint-comments": "^2.0.2",
      "eslint-plugin-filenames": "^1.2.0",
      "eslint-plugin-node": "^6.0.0",
      "eslint-plugin-promise": "^3.6.0",
      "mocha": "^5.0.0",
      "sbis3-json-generator": "git+ssh://git@git.sbis.ru:root/sbis3-json-generator.git#rc-3.18.300"
   },
   "engines": {
      "node": ">=8.9.3"
   }
}
