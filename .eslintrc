{
   "parserOptions": {
      "ecmaVersion": 8
   },
   "env": {
      "node": true,
      "es6": true,
      "mocha": true
   },
   "globals": {
      "require": true
   },
   "rules": {
      "keyword-spacing": 1,
      "no-cond-assign": 2,
      "no-constant-condition": 2,
      "no-console": 1,
      "no-debugger": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-irregular-whitespace": 2,
      "no-empty": 2,
      "no-extra-semi": 2,
      "no-negated-in-lhs": 2,
      "use-isnan": 2,
      "valid-typeof": 2,
      "no-caller": 2,
      "guard-for-in": 2,
      "no-eval": 2,
      "no-lone-blocks": 2,
      "no-redeclare": 2,
      "no-useless-call": 2,
      "no-with": 2,
      "no-delete-var": 2,
      "no-catch-shadow": 2,
      "no-undef": 2,
      "no-unused-vars": 2,
      "no-use-before-define": [
         2,
         "nofunc"
      ],
      "no-shadow-restricted-names": 2,
      "no-extra-bind": 2,
      "semi": 2,
      "new-cap": 2,
      "wrap-iife": [
         2,
         "any"
      ],
      "comma-style": [
         1,
         "last"
      ],
      "no-mixed-spaces-and-tabs": 1,
      "radix": 1,
      "vars-on-top": 0,
      "no-new-wrappers": 1,
      "max-nested-callbacks": [
         1,
         4
      ],
      "curly": 1,
      "operator-linebreak": 1,
      "no-obj-calls": 1,
      "no-sparse-arrays": 1,
      "no-unexpected-multiline": 1,
      "no-unreachable": 1,
      "quotes": [
         1,
         "single",
         "avoid-escape"
      ],
      "lines-around-comment": [
         1,
         {
            "beforeBlockComment": true,
            "beforeLineComment": true,
            "allowBlockStart": true
         }
      ],
      "new-parens": 1,
      "eol-last": 1,
      "indent": [
         1,
         3,
         {
            "SwitchCase": 1
         }
      ],
      "semi-spacing": [
         1,
         {
            "before": false,
            "after": true
         }
      ],
      "space-unary-ops": [
         1,
         {
            "words": true,
            "nonwords": false
         }
      ],
      "comma-spacing": [
         1,
         {
            "before": false,
            "after": true
         }
      ],
      "key-spacing": [
         1,
         {
            "beforeColon": false,
            "afterColon": true
         }
      ],
      "space-infix-ops": 1,
      "space-in-parens": [
         1,
         "never"
      ],
      "space-before-blocks": 1,
      "no-spaced-func": 1,
      "space-before-function-paren": [
         1,
         "never"
      ],
      "brace-style": 1,
      "id-match": [
         1,
         "^([$_A-Za-z][a-zA-Z0-9]{0,31}|[A-Z][A-Z0-9_]{2,31})$",
         {
            "properties": true
         }
      ],
      "strict": [
         1,
         "global"
      ],
      "no-sync": [
         1,
         {
            "allowAtRootLevel": true
         }
      ],
      "handle-callback-err": 1,
      "no-buffer-constructor": 1,
      "no-new-require": 1,
      "no-path-concat": 1,
      "no-var": 1,
      "no-class-assign": 1,
      "arrow-spacing": [
         1,
         {
            "before": true,
            "after": true
         }
      ],
      "constructor-super": 1,
      "no-confusing-arrow": 1,
      "no-const-assign": 1,
      "no-dupe-class-members": 1,
      "no-duplicate-imports": 1,
      "no-new-symbol": 1,
      "no-this-before-super": 1,
      "no-useless-computed-key": 1,
      "no-useless-constructor": 1,
      "no-useless-rename": 1,
      "prefer-const": 1,
      "require-yield": 1,
      "no-mixed-operators": 1,
      "no-useless-escape": 1,
      "no-unused-labels": 1,
      "no-self-assign": 1,
      "no-octal": 1,
      "no-global-assign": 1,
      "no-fallthrough": 1,
      "no-empty-pattern": 1,
      "no-case-declarations": 1,
      "no-unsafe-negation": 1,
      "no-unsafe-finally": 1,
      "no-regex-spaces": 1,
      "no-invalid-regexp": 1,
      "no-inner-declarations": 1,
      "no-func-assign": 1,
      "no-extra-boolean-cast": 1,
      "no-ex-assign": 1,
      "no-control-regex": 1,
      "no-compare-neg-zero": 1,
      "no-await-in-loop": 1,
      "getter-return": 1,
      "for-direction": 1,
      "no-extra-parens": 1,
      "no-template-curly-in-string": 1,
      "eqeqeq": 1,
      "no-alert": 1,
      "require-await": 1,
      "prefer-promise-reject-errors": 1,
      "no-useless-concat": 1,
      "no-unused-expressions": 1,
      "no-unmodified-loop-condition": 1,
      "no-throw-literal": 1,
      "no-sequences": 1,
      "no-self-compare": 1,
      "no-return-await": 1,
      "no-return-assign": 1,
      "no-proto": 1,
      "no-param-reassign": 1,
      "no-octal-escape": 1,
      "no-new-func": 1,
      "no-new": 1,
      "no-multi-str": 1,
      "no-loop-func": 1,
      "no-labels": 1,
      "no-invalid-this": 1,
      "no-implied-eval": 1,
      "no-floating-decimal": 1,
      "no-extend-native": 1,
      "no-eq-null": 1,
      "no-empty-function": 1,
      "dot-notation": 1,
      "dot-location": [
         1,
         "property"
      ],
      "default-case": 1,
      "consistent-return": 1,
      "class-methods-use-this": 1,
      "array-callback-return": 1,
      "accessor-pairs": 1,
      "no-shadow": 1,
      "no-undef-init": 1,
      "no-undefined": 1
   }
}
